{
  "name": "floatplaneapi",
  "version": "3.5.1",
  "description": "Floatplane API Specification",
  "main": "floatplane-openapi-specification.json",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.4.12",
    "openapi-to-postmanv2": "2.12.0",
    "redoc-cli": "0.13.0"
  },
  "scripts": {
    "clean": "rm -rf ./Docs",
    "validate": "$(npm bin)/openapi-generator validate -i floatplane-openapi-specification.json",
    "docs-all": "npm run clean && npm run docs-oag-html2 && npm run docs-redoc && npm run docs-rapidoc && npm run docs-postman",
    "docs-oag-html2": "$(npm bin)/openapi-generator-cli generate -i floatplane-openapi-specification.json -o Docs/OAG-html2/ -g html2 --global-property models,supportingFiles,apis=Activation:Auth:CDN:Comment:ConnectedAccounts:Content:Creator:CreatorSubscriptionPlan:Edges:FAQ:Iframe:Livestream:LoyaltyRewards:Playlist:Socket:Subscriptions:Sync:User:WebNotification",
    "docs-oag-dynamic-html": "$(npm bin)/openapi-generator-cli generate -i floatplane-openapi-specification.json -o Docs/OAG-html2/ -g dynamic-html --global-property models,supportingFiles,apis=Activation:Auth:CDN:Comment:ConnectedAccounts:Content:Creator:CreatorSubscriptionPlan:Edges:FAQ:Iframe:Livestream:LoyaltyRewards:Playlist:Socket:Subscriptions:Sync:User:WebNotification",
    "docs-redoc": "mkdir -p Docs/Redoc && $(npm bin)/redoc-cli bundle -o Docs/Redoc/redoc-static.html floatplane-openapi-specification.json",
    "docs-rapidoc": "mkdir -p Docs/Rapidoc && cp ./src/rapidoc.html ./Docs/Rapidoc/rapidoc.html",
    "docs-postman": "mkdir -p Docs/Postman-v2.1 && $(npm bin)/openapi2postmanv2 --spec floatplane-openapi-specification.json --output Docs/Postman-v2.1/floatplaneapi-postman2.1-collection.json --pretty -O folderStrategy=Tags,includeAuthInfoInExample=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jman012/FloatplaneAPI.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jman012/FloatplaneAPI/issues"
  },
  "homepage": "https://github.com/Jman012/FloatplaneAPI#readme"
}
